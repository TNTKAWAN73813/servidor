{
  "name": "API Minecraft",
  "image": "mcr.microsoft.com/vscode/devcontainers/python:3.11",
  "forwardPorts": [8080],
  "portsAttributes": {
    "8080": {
      "label": "API-NLS",
      "onAutoForward": "openPreview",
      "visibility": "public"
    }
  },
  "postStartCommand": "python3 /workspaces/servidor/minecraft/API-NLS.py",
  "customizations": {
    "codespaces": {
      "openFiles": ["minecraft/API-NLS.py"]
    }
  },
  "hostRequirements": {
    "cpus": 1,
    "memory": "1gb"
  },
  "features": {},
  "runArgs": ["--env", "GITHUB_CODESPACES_PORT_FORWARDING_DOMAIN=app.github.dev"]
}
